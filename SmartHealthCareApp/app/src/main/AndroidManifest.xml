<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.smarthealthcare.smarthealthcareapp">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <permission android:name="edu.smarthealthcare.smarthealthcareapp.permission.MAPS_RECEIVE" 
        android:protectionLevel="signature"/>
    <uses-permission android:name="edu.smarthealthcare.smarthealthcareapp.permission.MAPS_RECEIVE"/>
    <uses-permission android:name="com.google.android.providers.gsf.permissions.READ_GSERVICES"/>

    <uses-feature android:glEsVersion="0x00020000" android:required="true"/>
    
    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_login"
            android:parentActivityName=".GetStartActivity"></activity>
        <activity android:name=".GetStartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUpActivity"
            android:label="@string/app_signup"
            android:parentActivityName=".GetStartActivity"></activity>
        <activity
            android:name=".ActivityKitDetails"
            android:parentActivityName=".MainActivity">

        </activity>
        <activity android:name=".MapActivity"></activity>



        <meta-data android:name="com.google.android.maps.v2.API_KEY" android:value="@string/api_key"/>
        <meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version"/>

    </application>

</manifest>